
@prefix tree: <https://w3id.org/tree#> .
@prefix ldes: <https://w3id.org/ldes#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

@base <https://kgfixed.github.io/vocab.nerc.ac.uk/P01/2000_01_01_00_00_00_2000_12_31_00_00_00.ttl> .

<>
    a tree:Node ;
    
    ldes:retentionPolicy [
        a ldes:RetentionPolicy ;
        ldes:versionAmount 100
    ] .

<feed>
  a ldes:EventStream ;
  ldes:timestampPath dc:modified ;
  ldes:versionOfPath dc:isVersionOf ;
  ldes:immutable 'false'^^xsd:boolean ;
  ldes:pollingInterval '3600'^^xsd:integer ;
  tree:shape [
    a sh:NodeShape ;
    sh:nodeKind sh:IRI ;
    sh:property [
      sh:datatype xsd:string ;
      sh:minCount 1 ;
      sh:path dc:isVersionOf
    ],
    [
      sh:datatype xsd:dateTime ;
      sh:minCount 1 ;
      sh:path dc:modified
    ],
    [
      sh:datatype xsd:string ;
      sh:minCount 1 ;
      sh:path skos:altLabel
    ],
    [
      sh:datatype xsd:string ;
      sh:minCount 1 ;
      sh:path skos:prefLabel
    ],
    [
      sh:datatype xsd:string ;
      sh:minCount 1 ;
      sh:path skos:definition
    ],
    [
      sh:datatype xsd:string ;
      sh:minCount 1 ;
      sh:path skos:notation
    ];
    sh:targetClass skos:Concept
  ] ;
  tree:view <> ;
  .

<feed> tree:member <http://vocab.nerc.ac.uk/collection/P01/current/NMSKMSXT/1/> .
<http://vocab.nerc.ac.uk/collection/P01/current/NMSKMSXT/1/>
    a skos:Concept ;
    dc:isVersionOf <http://vocab.nerc.ac.uk/collection/P01/current/NMSKMSXT/>;
    dc:modified '2000-03-13T00:00:00'^^xsd:dateTime ;
    skos:altLabel 'NorMagSusBS'^^xsd:string ;
    skos:prefLabel 'Magnetic susceptibility (low frequency) per unit dry weight of sediment by magnetic susceptibility meter'@en ;
    skos:definition 'Unavailable'@en ;
    skos:notation 'SDN:P01::NMSKMSXT'^^xsd:string .

